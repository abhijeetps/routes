{
  "name": "@octokit/routes",
  "version": "0.0.0-development",
  "publishConfig": {
    "access": "public"
  },
  "description": "Machine-readable, always up-to-date GitHub REST API route specifications",
  "author": "Gregor Martynus (https://github.com/gr2m)",
  "main": "index.json",
  "bin": {
    "octokit-rest-routes": "bin/octokit-rest-routes.js"
  },
  "scripts": {
    "pretest": "standard",
    "test": "tap --coverage 'test/**/*-test.js'",
    "update": "node bin/octokit-rest-routes.js update",
    "check": "node bin/octokit-rest-routes.js check",
    "semantic-release": "semantic-release",
    "lint": "speccy lint openapi/index.json --skip openapi-tags",
    "start": "speccy serve openapi/index.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/octokit/routes.git"
  },
  "keywords": [
    "octokit"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octokit/routes/issues"
  },
  "homepage": "https://github.com/octokit/routes#readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "execa": "^1.0.0",
    "fs-extra": "^7.0.1",
    "gh-got": "^8.0.0",
    "got": "^9.3.2",
    "json-diff": "^0.5.3",
    "lodash": "^4.17.11",
    "markdown-table": "^1.1.2",
    "node-fetch": "^2.3.0",
    "normalize-url": "^4.0.0",
    "pluralize": "^7.0.0",
    "turndown": "^5.0.0",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "proxyquire": "^2.1.0",
    "semantic-release": "^15.11.0",
    "speccy": "^0.8.4",
    "standard": "^12.0.0",
    "tap": "^12.1.0"
  }
}
